<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="phoneBook">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="note">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element type="xs:string" name="name"/>
                            <xs:element type="xs:string" name="surname"/>
                            <xs:element type="xs:string" name="patronymic"/>
                            <xs:element type="xs:string" name="surnameAndInitials"/>
                            <xs:element type="xs:string" name="nickname"/>
                            <xs:element type="xs:string" name="comment"/>
                            <xs:element type="xs:string" name="group"/>
                            <xs:element type="xs:int" name="homePhone"/>
                            <xs:element type="xs:int" name="cellPhone"/>
                            <xs:element type="xs:string" name="secondCellPhone"/>
                            <xs:element type="xs:string" name="email"/>
                            <xs:element type="xs:string" name="skype"/>
                            <xs:element name="address">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element type="xs:int" name="index"/>
                                        <xs:element type="xs:string" name="city"/>
                                        <xs:element type="xs:string" name="street"/>
                                        <xs:element type="xs:byte" name="house"/>
                                        <xs:element type="xs:byte" name="apartment"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element type="xs:string" name="fullAddress"/>
                            <xs:element type="xs:string" name="registrationDate"/>
                            <xs:element type="xs:string" name="lastEditDate"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>